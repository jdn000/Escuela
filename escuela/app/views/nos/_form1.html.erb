
<%= form_for(@no) do |f| %>
 
  <%= f.label :Nombre_asignatura %>
  <%= f.text_field :nom_asig,class: 'form-control'  %>
  <%= f.label :Semestre %>
  <%= f.text_field :periodo,class: 'form-control'  %>
  <%= f.label :Rut_alumno %>
  <%= f.text_field :rut_al, class: 'form-control' %>
  <div class="col-xs-2"" form-horizontal">
  <%= f.label :N1 %>
  <%= f.text_field :X1, class: 'form-control' %>
  <%= f.label :N2 %>
  <%= f.text_field :X2, class: 'form-control' %>
  <%= f.label :N3 %>
  <%= f.text_field :X3, class: 'form-control' %>
  <%= f.label :N4 %>
  <%= f.text_field :X4, class: 'form-control' %>
  <%= f.label Promedio %>
  <%= f.text_field :XX, class: 'form-control' %>

  <%= f.submit "Ingresar", class: "btn btn-primary" %>

</div>
<% end %>